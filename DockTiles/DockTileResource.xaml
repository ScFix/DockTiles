<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:DockTiles"
                    xmlns:vm="clr-namespace:DockTiles.ViewModels"
                    xmlns:m="clr-namespace:DockTiles.Models"
                    xmlns:v="clr-namespace:DockTiles.Views">
    <DataTemplate DataType="{x:Type vm:LeafViewModel}">
        <v:LeafView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:RootNodeViewModel}">
        <v:DockTileRootView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:HorizontalSplitViewModel}">
        <v:HorizontalSplitView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:VerticalSplitViewModel}">
        <v:VeritcleSplitView />
    </DataTemplate>

</ResourceDictionary>